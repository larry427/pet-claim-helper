{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "claimiq.petclaimhelper.com" }],
      "destination": "/api/claim-iq"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "crons": [
    {
      "path": "/api/send-deadline-reminders",
      "schedule": "0 9 * * *"
    }
  ]
}
